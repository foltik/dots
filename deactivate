#!/usr/bin/env fish
set DOTS $(dirname $(realpath $(status -f)))
set PREFIX $DOTS/prefix

set -x PATH $PATH_NODOTS
set -x LD_LIBRARY_PATH $LD_LIBRARY_PATH_NODOTS
set -x XDG_CONFIG_HOME $XDG_CONFIG_HOME_NODOTS
set -x XDG_DATA_HOME $XDG_DATA_HOME_NODOTS
set -x RUSTUP_HOME $RUSTUP_HOME_NODOTS
set -x CARGO_HOME $CARGO_HOME_NODOTS
set -x DOTNET_ROOT $DOTNET_ROOT_NODOTS

set -e PATH_NODOTS
set -e LD_LIBRARY_PATH_NODOTS
set -e XDG_CONFIG_HOME_NODOTS
set -e XDG_DATA_HOME_NODOTS
set -e RUSTUP_HOME_NODOTS
set -e CARGO_HOME_NODOTS
set -e DOTNET_ROOT_NODOTS
set -e STARSHIP_CONFIG_NODOTS
set -e CODEX_HOME_NODOTS

exec bash
